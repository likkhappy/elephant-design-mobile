{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "keyofStringsOnly": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  // "typeRoots": ["node_modules/@types", "typings/**/*"],
  "files": ["typings/index.d.ts"],
  "exclude": ["node_modules"]
}
